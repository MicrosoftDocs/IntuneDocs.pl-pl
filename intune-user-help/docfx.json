{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "**/old-TOC.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/images/**",
          "**/*.png",
          "**/*.jpg",
          "**/*.gif",
          "**/*.bmp",
          "**/*.html",
          "**/*.css"
        ],
        "exclude": [
          "**/obj/**",
          "_themes/**"
        ]
      }
    ],
    "dest": "intune-user-help",
    "template": "docs.html",
    "globalMetadata": {
      "layout": "Conceptual",
      "disable_contribution_on_translation_quality": [
        "MT",
        "MTE75",
        "MTE95",
        "Blank"
      ],
      "breadcrumb_path": "/intune/bread/toc.json",
      "product_feedback_displaytext": "Intune Feedback",
      "product_feedback_url": "https://microsoftintune.uservoice.com/",
      "feedback_system": "GitHub",
      "feedback_github_repo": "MicrosoftDocs/Intunedocs.pl-pl",
      "contributors_to_exclude": [
        "herohua",
        "fenxu"
      ],
      "searchScope": [
        "Intune"
      ],
      "_op_documentIdPathDepotMapping": {
        "./": {
          "depot_name": "Azure.EndUser",
          "folder_relative_path_in_docset": "."
        }
      }
    },
    "markdownEngineName": "markdig"
  }
}